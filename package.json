{
  "author": "Niko Pettersen <niko.pettersen@gmail.com>",
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "axios": "^0.19.0-beta.1",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "env-var": "^3.4.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.7",
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.17",
    "vue-class-component": "^6.3.2",
    "vue-notification": "^1.3.13",
    "vue-progressbar": "^0.7.5",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "description": "Simple microblogging service written in TypeScript based on Express, MongoDB and Vue.js",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^3.0.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.8",
    "@types/mongoose": "^5.2.20",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-e2e-cypress": "^3.0.5",
    "@vue/cli-plugin-pwa": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "7.0.0-bridge.0",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.5",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^0.14.4",
    "vuex-class": "^0.3.1",
    "vuex-mock-store": "0.0.5"
  },
  "engines": {
    "node": "~10.12.0",
    "npm": "~6.4.1"
  },
  "homepage": "https://phoenix2k.github.io/vue-microblog-ts/",
  "keywords": [
    "express",
    "mongodb",
    "typescript",
    "vue"
  ],
  "license": "MIT",
  "name": "vue-microblog-ts",
  "postinstall": "npm run build",
  "repository": {
    "type": "git",
    "url": "https://github.com/Phoenix2k/vue-microblog-ts"
  },
  "scripts": {
    "deploy:docs": "scripts/deploy-docs.bash",
    "deploy:heroku": "scripts/deploy-heroku.bash",
    "dev:client": "vue-cli-service serve",
    "dev:server": "nodemon --project tsconfig-server.json",
    "dev:ui": "vue ui",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vue-cli-service build",
    "build:server": "tsc --project tsconfig-server.json",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "tslint --project tsconfig.json",
    "lint:fix": "npm run lint:fix:client && npm run lint:fix:server",
    "lint:fix:client": "tslint --fix --config tsconfig.json",
    "lint:fix:server": "tslint --fix --config tsconfig-server.json",
    "lint:server": "tslint --project tsconfig-server.json",
    "start": "node server/index.js",
    "test": "npm run test:e2e && npm run test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "version": "1.0.0"
}

{
  "author": "Niko Pettersen",
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "env-var": "^6.0.4",
    "express": "^4.17.1",
    "mongoose": "^5.9.7",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-notification": "^1.3.20",
    "vue-progressbar": "^0.7.5",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "description": "Small microblogging service written in TypeScript based on Express, Mongoose and Vue.js",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.4",
    "@types/jest": "^25.1.5",
    "@types/mongoose": "^5.7.8",
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-e2e-cypress": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-plugin-pwa": "^4.2.3",
    "@vue/cli-plugin-typescript": "^4.2.3",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.2.6",
    "canvas": "^2.6.1",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "filename-regex": "^2.0.1",
    "jest": "^25.2.6",
    "jest-haste-map": "^25.2.6",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "vue-class-component": "^7.2.3",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.4.0",
    "vuex-class": "^0.3.2",
    "vuex-mock-store": "0.0.8",
    "webpack-dev-middleware": "^3.7.2"
  },
  "engines": {
    "node": "~12.13.0",
    "npm": "~6.14.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "cypress/globals": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "plugins": [
      "cypress"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "homepage": "https://phoenix2k.github.io/vue-microblog-ts/",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/jest.setup.js"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "transform": {
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "keywords": [
    "cli",
    "express",
    "microblogging",
    "mongodb",
    "mongoose",
    "node",
    "service",
    "typescript",
    "vue"
  ],
  "license": "MIT",
  "name": "vue-microblog-ts",
  "nodemonConfig": {
    "exec": "ts-node --project tsconfig-server.json ./src/server",
    "ext": "ts",
    "ignore": [
      "public/*"
    ],
    "verbose": true,
    "watch": [
      "./src/server"
    ]
  },
  "postinstall": "npm run build",
  "prettier": {
    "arrowParens": "always",
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Phoenix2k/vue-microblog-ts"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "vue-cli-service build",
    "build:server": "tsc --project tsconfig-server.json",
    "deploy:docs": "scripts/deploy-docs.bash",
    "deploy:heroku": "scripts/deploy-heroku.bash",
    "dev:client": "vue-cli-service serve",
    "dev:server": "nodemon --project tsconfig-server.json",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "prettier": "prettier --write '**/*.{js,json,ts,tsx}'",
    "prettier:check": "prettier --check '**/*.{js,json,ts,tsx}'",
    "start": "node dist/server/index.js",
    "test": "npm run test:e2e && npm run test:unit",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test:unit": "vue-cli-service test:unit",
    "ui": "vue ui"
  },
  "version": "1.0.2"
}
